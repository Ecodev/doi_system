{
	"name": "fab/doi-system",
	"type": "typo3-cms-extension",
	"description": "DOI System - OAI server emulation. OAI stands for Open Archives Initiative, a protocol for Metadata Harvesting.",
	"homepage": "https://github.com/Ecodev/doi_system",
	"license": ["GPL-2.0-or-later"],
	"keywords": ["TYPO3 CMS", "DOI system", "OAI server"],
	"support": {
		"issues": "https://github.com/Ecodev/doi_system/issues"
	},
	"autoload": {
		"psr-4": {
			"Fab\\DoiSystem\\": "Classes/"
		}
	},
	"replace": {
		"fab/doi_system": "self.version"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "doi_system"
		}
	},
	"require": {
		"typo3/cms-core": "^11.5"
	}
}
